                if (_this.attr("web") == "1") {
                    _this.after(_this.prop("outerHTML").replace("<span", "<a").replace("</span>", "</a>")).remove();
                }
            });

            //资讯正文支持点击查看原图
            jQuery("#ContentBody img").wrap(function () {
                var _this = $(this);
                if (_this.hasAttribute("orginial_src")) {
                    var orginial_src = _this.getAttribute("orginial_src");
                    return '<a href="' + orginial_src + '" target="_blank"></a>';
                }
                return '';
            });

            //正文页增加机会文章模块功能
            var _op_ids = [];
            jQuery("#ContentBody .em_op_news").each(function () {
                var _this = jQuery(this);
                var _op_id = _this.attr("op_id");
                _op_ids.push(_op_id);
                _this.addClass("em_op_news" + _op_id);
            });
            function _initOPHTML(json) {
                var _opHTML = '';
                if (json.links.length > 0) {
                    _opHTML += '<div class="op_item">';
                    _opHTML += '<div class="op_title" style="color:' + json.title_color + '">' + json.title + '</div>';
                    _opHTML += '<ul>';
                    for (var i = 0; i < json.links.length; i++) {
                        _opHTML += '<li><a href="' + json.links[i].url + '">' + json.links[i].text + '</a></li>';
                    }
                    _opHTML += '</ul>';
                    _opHTML += '</div>';
                    return _opHTML;
                }
                return _opHTML;
            }
            if (_op_ids.length > 0) {
                jQuery.ajax({
                    //url: "//newsinfo.test.emapd.com/
                    url: "//newsinfo.eastmoney.com/kuaixun/v2/api/content/op_modules?op_ids=" + _op_ids.toString() + "&callback=?",
                    type: "get",
                    dataType: "jsonp",
                    data: {},
                    success: function (json) {
                        for (var i = 0; i < json.Result.length; i++) {
                            var _op_id = json.Result[i].op_id;
                            var _this = jQuery("#ContentBody .em_op_news" + _op_id);
                            var _opHTML = _initOPHTML(json.Result[i]);
                            if (_opHTML.length > 0) {
                                _this.html(_opHTML);
                                _this.show();
                            }
                        }
                    }
                })
            }
        })

        var emshare_news = new emshare2020({
            
            dom: "#shareTop,#shareTo"// 几个分享按钮的容器    可选    默认“.emshare2020”
        });

        emshare_news.qrcode({
            width: 100,// 二维码宽度 可选 默认135
            
            link: "https://wap.eastmoney.com/info/detail/202005081478962798",
            domid: "wapqrcode"
            
        });

        window.weiboOpt = {
            source: "东方财富网",
            sourceUrl: "http://www.eastmoney.com/",
            Appkey: "3050999700",
            RalateUid: "1801487174",
            weibolink: "http://weibo.com/dfcfw"
        }

        var shareRight = '<ul class="attWeiBo">' +
                '	<li class="weibo npr"><a href="' + weiboOpt.weibolink + '" target="_blank"><i></i>' + weiboOpt.source + '</a></li>' +
                '	<li class="guanzhu mr6"><iframe width="63" height="24" frameborder="0" allowtransparency="true" marginwidth="0" marginheight="0" scrolling="no" border="0" src="//widget.weibo.com/relationship/followbutton.php?language=zh_cn&width=63&height=24&uid=' + weiboOpt.RalateUid + '&style=1&btn=red&dpc=1"></iframe></li>' +
                '	<li class="guanzhu"><a class="wxBtn" onmouseover="document.getElementById(\'weixin_tips\').style.display=\'block\'" onmouseout="document.getElementById(\'weixin_tips\').style.display=\'none\'"><div id="weixin_tips" class="wxtip wx_underfined"></div></a></li>' +
                '</ul>';

        var shareTo = jQuery("#shareTo");//分享没有禁用
        var noShare = jQuery("#noShare");//禁用分享的时候，只有右侧
        if (shareTo.length > 0) {
            shareTo.append(shareRight);//分享启用的时候追加

            //调整左侧插件定位
            jQuery("#shareTo .emshare2020_buttonbox").css({ "float": "left", "height": "40px", "line-height": "40px" });
            jQuery("#shareTo .emshare2020_buttonbox a,#shareTo .emshare2020_buttonbox span").css({ "margin-top": "11px" });
        }
        if (noShare.length > 0) {
            noShare.html(shareRight);//禁用分享的时候，只有右侧
        }

    </script>



